# https://www.codechef.com/START126D/problems/ACPLZ

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input = new Scanner(System.in);
		int t = input.nextInt();
		if (t>30) {
		    System.out.println("YES");
		}
		else {
		    System.out.println("NO");
		}

	}
}
