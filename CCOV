# https://www.codechef.com/problems/CCOV

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner input = new Scanner(System.in);
	    int speed = input.nextInt();
	    if (speed>40) {
	        System.out.println("YES");
	    }
	    else {
	        System.out.println("NO");
	    }
	}
}
