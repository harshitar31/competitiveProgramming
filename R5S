# https://www.codechef.com/START128D/problems/R5S

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input = new Scanner(System.in);
		int r = input.nextInt();
		int c = input.nextInt();
		if (r+c>=2000) {
		    System.out.println("YES");
		}
		else {
		    System.out.println("NO");
		}

	}
}
